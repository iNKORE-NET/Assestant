{
    "name": "@assestant/runtime",
    "version": "0.0.2",
    "description": "Runtime package for Assestant - Include assets in your NPM package with your favorite build tool!",
    "type": "module",
    "scripts": 
    {
        "dev": "rollup --config rollup.config.mjs --watch --environment MODE:dev",
        "build": "rollup -c rollup.config.mjs --environment MODE:prod"
    },
    "exports": 
    {
        ".": 
        {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./internal": 
        {
            "types": "./dist/internal/index.d.ts",
            "default": "./dist/internal/index.js"
        }
    },
    "devDependencies": 
    {
    },
    "author": "notyoojun",
    "license": "LGPL-3.0-or-later",
    "repository": 
    {
        "type": "git",
        "url": "https://github.com/iNKORE-NET/Assestant",
        "directory": "packages/runtime"
    },
    "homepage": "https://docs.inkore.net/assestant",
    "bugs": "https://github.com/iNKORE-NET/Assestant/issues",
    "publishConfig": 
    {
        "access": "public"
    },
    "files": 
    [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "keywords": 
    [
        "bundler",
        "rollup",
        "webpack",
        "vite",
        "esbuild",
        "assets",
        "images",
        "fonts",
        "css",
        "scss",
        "sass",
        "less",
        "asset",
        "assets",
        "assestant",
        "website"
    ]
}